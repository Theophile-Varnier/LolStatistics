@using LolStatistics.Web
@using LolStatistics.Model.Static
@model LolStatistics.Web.Models.GameHistoryViewModel

@{
    ViewBag.Title = "Home Page";
}
<nav class="text-center">
    <ul class="pagination">
        <li>
            <a aria-label="Previous">
                <span aria-hidden="true">&laquo;</span>
            </a>
        </li>
        @for (int i = 0; i < Model.PageNumber; i++)
        {
            <li><a page="@(i)" href="#top">@(i + 1)</a></li>
        }
        <li>
            <a aria-label="Next">
                <span aria-hidden="true">&raquo;</span>
            </a>
        </li>
    </ul>
</nav>
<div class="match-history">
    @Html.Partial("Partial/MatchHistory", Model)
</div>
<nav class="text-center">
    <ul class="pagination">
        <li>
            <a page="previous">
                <span aria-hidden="true">&laquo;</span>
            </a>
        </li>
        @for (int i = 0; i < Model.PageNumber; i++)
        {
            <li><a page="@(i)" href="#top">@(i + 1)</a></li>
        }
        <li>
            <a page="next">
                <span aria-hidden="true">&raquo;</span>
            </a>
        </li>
    </ul>
</nav>
@section scripts{
    @Scripts.Render("~/Scripts/Index.js")
}